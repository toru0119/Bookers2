<% if @book.errors.any? %>
  <%= @book.errors.count %>件のerrorが発生しました
  <% @book.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
<% end %>
<%= render'books/list',{book: @book, user: @user} %>
    <div class="col-md-8 offset-md-1">
      <%= render 'searchs/form' %>
      <h2>Books</h2>
      <%= render 'books/index',{books:@books, user:@user} %>
    </div>
